{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "TypeScript Build",
			"type": "shell",
			"command": "tsc",
			"args": [
				"-p",
				"."
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": []
		},
		{
			"label": "Install Dependencies",
			"type": "shell",
			"command": "npm",
			"args": [
				"install"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": []
		}
	]
}
